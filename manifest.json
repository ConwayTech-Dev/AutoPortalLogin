{
  "manifest_version": 3,
  "name": "MyPolyAutoLogin",
  "version": "1.0",
  "icons": {
    "16": "poly_logo.png",
    "48": "poly_logo.png",
    "128": "poly_logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://polytechnic.myschoolapp.com/*","https://accounts.google.com/*", "https://*.myschoolapp.com/*", "https://*.blackbaud.com/*"],
      "js": ["src/Content.js"],
      "run_at": "document_start"
      
    }
  ],
  "action": {
    "default_popup": "Popup.html"
  },
  "permissions": ["activeTab"]
}
